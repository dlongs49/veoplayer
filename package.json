{
  "name": "veoplayer",
  "version": "1.0.1",
  "description": "A native JavaScript concise video player",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "node ./test/index.js",
    "dev:serve": "node ./scripts/server.cjs",
    "dev": "node ./scripts/rollup.watch.cjs",
    "out:css": "node ./packages/watch.js",
    "watch:css": "sass --watch ./packages/style:./packages/style",
    "build:css": "sass --style compressed ./packages/style:./packages/style",
    "serve": "rollup -c -w"
  },
  "keywords": [
    "vidoe",
    "player"
  ],
  "author": "dinglong",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@rollup/plugin-commonjs": "^25.0.2",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-strip": "^3.0.2",
    "autoprefixer": "^10.4.14",
    "cssnano": "^6.0.1",
    "fs-extra": "^11.1.1",
    "hls.js": "^1.4.6",
    "rollup": "^3.24.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-serve": "^2.0.2",
    "rollup-plugin-terser": "^7.0.2"
  }
}